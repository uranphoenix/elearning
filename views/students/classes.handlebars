<h2>{{student.first_name}}</h2>
<table cellspacing="0" cellpadding="0">
    <thead>
    <tr>
        <th>Class ID</th>
        <th>Class name</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    {{#each student.classes}}
        <tr>
            <td>{{class_id}}</td>
            <td>{{class_title}}</td>
            <td><a href="/classes/{{class_id}}/lessons">View lessons</a></td>
        </tr>
    {{/each}}
    </tbody>
</table>